{
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./"],
          "extensions": [".ios.ts", ".android.ts", ".ts", ".ios.tsx", ".android.tsx", ".tsx", ".jsx", ".js", ".json"],
          "alias": {
            "@api": "./src/api",
            "@assets": "./src/assets",
            "@components": "./src/components",
            "@config": "./src/config",
            "@screens": "./src/screens",
            "@utils": "./src/utils",
            "@model": "./src/models",
            "@navigation": "./src/navigation",
            "@stores": "./src/stores",
            "@hooks": "./src/hooks",
            "@schemas": "./src/schemas",
            "@constants": "./src/constants",
            "@common": "./src/common"
          }
        }
      ]
    ]
  }
  